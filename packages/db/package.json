{
  "name": "@kuman/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./client": {
      "default": "./src/client.ts"
    },
    "./schema": {
      "default": "./src/schema.ts"
    },
    "./supabase": {
      "default": "./src/supabase.ts"
    },
    "./enums": {
      "default": "./src/enums/index.ts"
    },
    "./utils": {
      "default": "./src/utils/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache dist node_modules",
    "dev": "tsc --watch",
    "format": "pnpm --filter @kuman/prettier-config run format",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio --config drizzle.config.ts",
    "generate": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.55.0",
    "argon2": "^0.43.1",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.44.1",
    "pg": "^8.16.0",
    "ulidx": "^2.4.1"
  },
  "devDependencies": {
    "@kuman/eslint-config": "workspace:*",
    "@kuman/prettier-config": "workspace:*",
    "@kuman/shared": "workspace:*",
    "@kuman/tsconfig": "workspace:*",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.15.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@kuman/prettier-config"
}
