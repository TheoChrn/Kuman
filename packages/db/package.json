{
  "name": "@kuman/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache dist node_modules",
    "dev": "tsc",
    "format": "pnpm --filter @kuman/prettier-config run format",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.1",
    "pg": "^8.16.0",
    "ulidx": "^2.4.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@kuman/eslint-config": "workspace:*",
    "@kuman/prettier-config": "workspace:*",
    "@kuman/tsconfig": "workspace:*",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.15.4",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@kuman/prettier-config"
}
